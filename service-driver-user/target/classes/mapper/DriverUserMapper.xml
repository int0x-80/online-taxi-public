<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wang.service.driver.user.mapper.DriverUserMapper">

    <select id="selectDriverUserByCityCode" parameterType="string" resultType="int">
        select count(*) from v_city_driver_user_work_status where city_code = #{cityCode}
    </select>


</mapper>